#include <stdio.h>

int main(void)
{
	int integer = 5;
	float singlePrecision = 5.5;
	double doublePrecision = 6.66;
	char singleChar = 'J';

	
	printf("Your integer is %d \n", integer);
	printf("Your float is %f \n", singlePrecision);
	printf("Your double is %lf \n", doublePrecision);
	printf("Your first is %c \n", singleChar);

	
	return 0;
}
